  <Layout layout_name="PartEd_SavePreset_Dialog" layout_guid="1eddcc2f-45ba-49f6-9059-187e14d96226">
    <Window stylename="default_window" override_flow_index="9999999" height="215dpi_px" width="500dpi_px" localise_title="ParticleEd_NewPresetTitle" window_flags="CloseButton, DockingDisabled, DesktopTransferDisabled">
      <Client>
        <ChildGadgets>
          <Grid stylename="grid_container" override_flow_index="9999999" left="8dpi_px" right="8dpi_px" grid_rowlayout="['48dpi_px', '24dpi_px', '1*', '28dpi_px']">
            <Cell row="0" column="0" stylename="grid_container_panel" flow_index="0">
              <ChildGadgets>
                <Label stylename="label" override_flow_index="9999999" height="16px" width="244px" left="0px" localise_caption="ParticleEd_SaveNewPreset" />
              </ChildGadgets>
            </Cell>
            <Cell row="1" column="0" stylename="grid_container_panel" flow_index="0">
              <ChildGadgets>
                <TextBox stylename="text_box" layout_id="preset_name" />
              </ChildGadgets>
            </Cell>
            <Cell row="2" column="0" stylename="grid_container_panel" flow_index="0">
              <ChildGadgets>
                <Label stylename="label" override_flow_index="9999999" height="" width="" left="0px" localise_caption="ParticleEd_NewPresetMsg" word_wrap="True" />
              </ChildGadgets>
            </Cell>
            <Cell row="3" column="0" stylename="grid_container_panel" flow_index="0">
              <ChildGadgets>
                <StackPanel stylename="stack_panel" override_flow_index="9999999" stack_order="RightToLeft">
                  <ChildGadgets>
                    <Button stylename="button_up" layout_id="button_save" override_flow_index="9999999" width="140dpi_px" right="0px" localise_name="Button_Save" />
                    <Button stylename="button_up" layout_id="button_cancel" override_flow_index="9999999" width="140dpi_px" right="150px" margin_right="10dpi_px" localise_name="Button_Cancel" />
                  </ChildGadgets>
                </StackPanel>
              </ChildGadgets>
            </Cell>
          </Grid>
        </ChildGadgets>
      </Client>
      <Layout layout_name="BasicWindowFrame" layout_guid="f0695f87-de65-4a48-a3a8-9bd5cb5a8d8c" />
    </Window>
  </Layout>
