        </ComboBox>
        <Button stylename="button_up" layout_id="button_copygml" layout_guid="6432e83d-38dc-42bb-bac2-2020641e5d4a" override_flow_index="9999999" localise_tooltip="ParticleEd_CopyGMLToClipboard" width="24dpi_px" left="422px" margin_left="4dpi_px" margin_right="4dpi_px" icon_flags="Centre" font_icon="[&quot;asset_type_icon_font&quot;, &quot;AT_icon_scripts&quot;]" vector_icon_size="18dpi_px" />
