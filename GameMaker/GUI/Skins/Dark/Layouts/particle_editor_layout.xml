        <ComboBox stylename="combobox_panel" layout_id="draw_order" layout_guid="4b25e57a-8943-491a-b63a-bee8aa866aa1" override_flow_index="9999999" left="294px" margin_left="8dpi_px" margin_right="4dpi_px" bind_property="drawOrder">
