                                <StackPanel stylename="stack_panel" height="20dpi_px" right="6dpi_px" stack_order="RightToLeft">
                                  <ChildGadgets>
                                    <Button stylename="button_up" layout_id="files_right" layout_guid="7563d993-fe9f-4144-8acb-6b92b6b36c34" localise_tooltip="SourceControlGit_PageRight" height="20dpi_px" width="34dpi_px" right="0px" margin_left="6dpi_px" icon_flags="Stretch, Centre, KeepAspectRatio" icon_render_flags="FlipHorizontal" icon="ArrowLeft.png" />
