<?xml version="1.0" encoding="utf-8"?>
<Layouts>
  <Layout layout_name="AIAsst_ChatListHeader" layout_guid="2c73e1f1-3fb8-4aed-95ff-fcf4e93bc70d">
    <Grid stylename="chat_history_header_grid" layout_id="header_grid" top="0px" colour="aiasst_grey0">
      <Cell row="0" column="0" stylename="grid_container_panel" flow_index="0">
        <ChildGadgets>
          <CheckBox stylename="checkbox_stack" layout_id="select_check" layout_guid="7cf126ec-167f-426c-9e92-3e3c08f67302" override_flow_index="9999999" />
        </ChildGadgets>
      </Cell>
      <Cell row="0" column="1" stylename="grid_container_panel" flow_index="0">
        <ChildGadgets>
          <Label stylename="label" override_flow_index="9999999" height="16px" width="24px" left="8dpi_px" localise_caption="AIAsst_Title" />
        </ChildGadgets>
      </Cell>
      <Cell row="0" column="2" stylename="grid_container_panel" flow_index="0">
        <ChildGadgets>
          <StackPanel stylename="stack_panel" layout_id="lastmod_stack" stack_resize="ResizeHeight" left="0px">
            <ChildGadgets>
              <Label stylename="label" layout_id="lastmod_label" layout_guid="85a5e3fa-8a27-465d-87ee-385a58d4d152" override_flow_index="9999999" height="16px" width="75px" left="0px" localise_caption="AIAsst_LastModified" />
              <Button stylename="button_up" layout_id="btn_sortmode" layout_guid="82ffd8c0-7765-4fe9-a55a-b892556f1491" override_flow_index="9999999" height="20dpi_px" width="14dpi_px" left="77px" margin_left="2dpi_px" icon_flags="Centre" font_icon="[&quot;ai_icon_font&quot;, &quot;icon_ai_chevron_down&quot;]" vector_icon_size="9dpi_px" />
            </ChildGadgets>
          </StackPanel>
        </ChildGadgets>
      </Cell>
      <Cell row="0" column="3" stylename="grid_container_panel" flow_index="0">
        <ChildGadgets>
          <Label stylename="label" override_flow_index="9999999" height="16px" width="40px" left="0px" localise_caption="AIAsst_Shared" />
        </ChildGadgets>
      </Cell>
    </Grid>
  </Layout>
  <Layout layout_name="AIAsst_History_Empty2" layout_guid="3322713b-5236-448f-9229-86182459c8d4">
    <Grid stylename="grid_container" override_flow_index="9999999" grid_rowlayout="['32dpi_px', '1*']" grid_columnlayout="['32dpi_px']">
      <Cell row="1" column="0" stylename="grid_container_panel" flow_index="0">
        <ChildGadgets>
          <StackPanel stylename="stack_panel" override_flow_index="9999999" stack_order="TopToBottom" stack_resize="ResizeAll">
            <ChildGadgets>
              <Label stylename="aiasst_chatempty_label" override_flow_index="9999999" height="16px" width="117px" top="0px" caption="Chat history is empty" />
              <Button stylename="button_up" layout_id="btn_newchat" layout_guid="ff975a04-879f-4ad0-a517-aeb6ec3006bb" override_flow_index="9999999" height="30px" width="150dpi_px" top="28px" margin_top="12dpi_px" margin_bottom="12dpi_px">
                <ChildGadgets>
                  <Grid stylename="grid_container" override_flow_index="9999999" grid_columnlayout="['1*', '28dpi_px']">
                    <Cell row="0" column="0" stylename="grid_container_panel" flow_index="0">
                      <ChildGadgets>
                        <Label stylename="label" override_flow_index="9999999" height="16px" width="84px" localise_caption="AIAsst_StartNewChat" />
                      </ChildGadgets>
                    </Cell>
                    <Cell row="0" column="1" stylename="grid_container_panel" flow_index="0">
                      <ChildGadgets>
                        <Image stylename="image" override_flow_index="9999999" height="20dpi_px" width="16dpi_px" left="0px" image_flags="Centre" font_icon="[&quot;ai_icon_font&quot;, &quot;icon_ai_send&quot;]" vector_icon_size="16dpi_px" />
                      </ChildGadgets>
                    </Cell>
                  </Grid>
                </ChildGadgets>
              </Button>
            </ChildGadgets>
          </StackPanel>
        </ChildGadgets>
      </Cell>
    </Grid>
  </Layout>
