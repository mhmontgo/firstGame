						{
							transition : "Lerp",
							property: "AnimatedScale",
							destination: [1, 1],
							time : 200,
							relative: "false"
						}
					],
	
    "progressbar_test_anim" : 
					[
						{
							transition : "Lerp",
							property: "ProgressValue",
							destination: 1,
							time : 20000,
							relative: "false"
						}
					],
    
