{
    "close_window": [
                        {
                            transition: "Lerp",
                            property: "RenderTargetScale",
                            destination: 0.5,
                            time : 150,
                            relative: "false"
                        },
                        { 
                            transition : "Lerp",
                            property: "RenderTargetColour",
                            destination: "#00000000",
                            time : 150,
                            relative: "false"
                        }
                    ],

	"minimize_window": [
						{
							transition: "Lerp",
							property: "ClientHeight",
							destination: 0,
							time : 250,
							relative: "false"
						}
                    ],

	"minimize_restore_window": [
						{
							transition: "Lerp",
							property: "animate_height",
							destination_property: "RestoreHeight",
							time : 250,
							relative: "false"
						}
                    ],

	"expand_treeview" : [
						{
							time : 500
						}
					],
	"collapse_treeview" : [
						{
							time : 500
						}
					],

	"tab_drag_release" : 
					[
						{
							transition : "Lerp",
							property: "AnimatedOffset",
							destination: [0, 0],
							time : 200,
							relative: "false"
						}
					],

	"tab_reorder" : 
					[
						{
							transition : "Lerp",
							property: "AnimatedOffset",
							destination: [0, 0],
							time : 200,
							relative: "false"
